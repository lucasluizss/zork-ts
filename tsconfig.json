{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"target": "es2021",
		"strict": true,
		"sourceMap": true,
		"lib": ["es2021"],
		"skipLibCheck": true,
		"rootDir": "src",
		"outDir": "dist",
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"strictPropertyInitialization": false
	}
}
